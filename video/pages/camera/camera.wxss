/* 模板引用样式--start */
.box {
    display: flex;
    flex-direction: row;
    flex-wrap: nowrap;
    justify-content:space-around;
}
.btn{
  width: 200rpx;
  height: 120rpx;
  margin-top: 20rpx;
  background-color: rgba(0, 0, 0, 0);
  color: white;
  border-radius: 0rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
}

 .btnImg{
  width: 50rpx;
  height: 50rpx;
}
 .btnImg2{
  width: 200rpx;
  height: 200rpx;
}

.btn::after {
  border: 0; 
}
/* 模板引用样式--end */
.image{
  width: 1000rpx;
  height: 1000rpx;
}
